<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #fff3c6;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eoc.ui.LeaderboardController">
   <children>
      <Label layoutX="211.0" layoutY="35.0" prefHeight="17.0" prefWidth="291.0" text="Leaderboard" textFill="#374232">
         <font>
            <Font name="Droid Serif Bold Italic" size="27.0" />
         </font>
      </Label>
      <Label layoutX="33.0" layoutY="93.0" prefHeight="17.0" prefWidth="264.0" text="*Based on latest time played, for each player" textFill="#660707">
         <font>
            <Font name="Droid Serif" size="12.0" />
         </font>
      </Label>
      <ImageView blendMode="MULTIPLY" fitHeight="60.0" fitWidth="69.0" layoutX="531.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@logo.png" />
         </image>
      </ImageView>
      <Button fx:id="backButton" layoutX="14.0" layoutY="11.0" mnemonicParsing="false" onAction="#onBackButtonClick" prefHeight="25.0" prefWidth="69.0" style="-fx-background-color: #4c5d45;" text="Back" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <TextArea fx:id="leaderboardTextArea" blendMode="MULTIPLY" editable="false" layoutX="39.0" layoutY="123.0" prefHeight="248.0" prefWidth="525.0">
