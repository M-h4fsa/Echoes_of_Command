<AnchorPane prefHeight="150.0" prefWidth="483.0" style="-fx-background-color: #eadcc7;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eoc.ui.UsernameController">
   <children>
      <TextField fx:id="usernameField" layoutX="218.0" layoutY="62.0" prefHeight="25.0" prefWidth="203.0" />
      <Button fx:id="submitButton" layoutX="349.0" layoutY="98.0" mnemonicParsing="false" onAction="#handleSubmit" prefHeight="25.0" prefWidth="73.0" style="-fx-background-color: #fff3c6;" text="Submit" />
