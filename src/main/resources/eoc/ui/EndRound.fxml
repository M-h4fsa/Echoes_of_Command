<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="200.0" prefWidth="500.0" style="-fx-background-color: #eadcc7;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eoc.ui.EndRoundController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="17.0" prefWidth="193.0" text="Round ended!" textFill="#374232">
         <font>
            <Font name="Droid Serif Bold Italic" size="19.0" />
         </font>
      </Label>
      <Button fx:id="quitButton" layoutX="182.0" layoutY="150.0" mnemonicParsing="false" onAction="#onQuitButtonClick" prefHeight="25.0" prefWidth="136.0" style="-fx-background-color: #4c5d45;" text="Quit" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <ImageView blendMode="MULTIPLY" fitHeight="61.0" fitWidth="72.0" layoutX="418.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@logo.png" />
         </image>
      </ImageView>
      <Label fx:id="scoreLabel" layoutX="351.0" layoutY="38.0" prefHeight="17.0" prefWidth="100.0" text="Score: 0 / 0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="timeLabel" layoutX="204.0" layoutY="38.0" prefHeight="17.0" prefWidth="100.0" text="Time: N/A">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="216.0" layoutY="17.0" text="Time spent:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="375.0" layoutY="17.0" text="Score:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <ImageView blendMode="MULTIPLY" fitHeight="122.0" fitWidth="157.0" layoutY="38.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@eor.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>