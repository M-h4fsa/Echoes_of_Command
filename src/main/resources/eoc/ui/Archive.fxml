<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eoc.ui.ArchiveController">
    <children>
        <ImageView fitHeight="408.0" fitWidth="606.0" layoutX="-2.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@pattern.png" />
            </image>
        </ImageView>
        <Button fx:id="searchButton" layoutX="457.0" layoutY="32.0" mnemonicParsing="false" onAction="#onSearchButtonClick" prefHeight="25.0" prefWidth="103.0" style="-fx-background-color: #fff3c6;" text="Search" />
        <Button fx:id="backButton" layoutX="32.0" layoutY="363.0" mnemonicParsing="false" onAction="#onBackButtonClick" prefHeight="25.0" prefWidth="70.0" style="-fx-background-color: #4c5d45;" text="Back" textFill="WHITE">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Button>
        <TextField fx:id="searchField" layoutX="32.0" layoutY="32.0" prefHeight="25.0" prefWidth="410.0" />
        <ImageView blendMode="MULTIPLY" fitHeight="66.0" fitWidth="70.0" layoutX="534.0" layoutY="352.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@logo.png" />
            </image>
        </ImageView>
        <TextArea fx:id="resultArea" blendMode="MULTIPLY" layoutX="32.0" layoutY="80.0" prefHeight="267.0" prefWidth="531.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></TextArea>
    </children>
</AnchorPane>
